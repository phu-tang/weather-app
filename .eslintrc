{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": ["react-app", "react-app/jest", "prettier"],
  "plugins": ["react", "prettier"],
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
          "printWidth": 120,
          "trailingComma": "es5",
          "singleQuote": true,
          "semi": true,
          "no-mixed-spaces-and-tabs": ["error", "smart-tabs"],
          "no-unused-vars": ["error"],
          "endOfLine":"auto"
      }
    ],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  }
}